extends layout
block content
    .container
        .row
            .col-md-3
                .row
                    include includes/list
                    +AddList(IPList)
            .col-md-5#form
                include includes/form
                .panel.panel-default
                    .panel-heading
                        h3.panel-title.text-center #{form.title}
                    .panel-body
                        form.form-horizontal
                            each item,i in form.data
                                +addRow(item.id,item.name,item.type,item.value,item.readonly)
                            .form-group
                                .col-md-offset-4.col-md-4
                                    button.btn.btn-primary.btn-block(id='submit' type="button" onclick="submitDxzp(this)")
                                        span(class="glyphicon glyphicon-ok" aria-hidden="true")
                                            | &nbsp
                                        span
                                            | 提交
            .col-md-4(id="dxzpModify" style="display:none")
                .panel.panel-default
                    .panel-heading
                        h3.panel-title.text-center
                            | 黑灰名单维护结果
                    .panel-body(id="dxzpResult")